<%= form_tag("update", method: "post", id: params[:id]) do %>
  <%= label_tag(:topic, "Topic of the mock debate:") %>
  <%= text_field_tag(:topic) %>
  <%= label_tag(:time, "Time of the mock debate:") %>
  <%= text_field_tag(:time) %>
  <%= label_tag(:location, "location of the mock debate:") %>
  <%= text_field_tag(:location) %>
  <%= submit_tag("Modify") %>
<% end %>

<%= button_to "Delete This Mock Debate", action: "update", method: "get", :delete => "delete" %>

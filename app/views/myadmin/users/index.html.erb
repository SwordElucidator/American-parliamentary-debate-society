<div class = "container">
  <%= render 'navigation'%>
</div>
<div class="container">
  <%= render 'myadmin/dashboard/catalog' %>
  <div class="col-sm-9">
    <h1>Listing Users</h1>
    <table class = "table table-condensed">
      <thead>
        <tr>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>User ID</td>
          <td>User Email</td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td>
                <%= link_to dashboard_users_show_path(:id => user.id) do %>
                  <span class="glyphicon glyphicon-floppy-disk"></span>
                <% end %>
            </td>
            <td>
                <%= link_to dashboard_users_new_path(:id => user.id) do %>
                  <span class="glyphicon glyphicon-plus"></span>
                <% end %>
            </td>
            <td>
                <%= link_to dashboard_users_edit_path(:id => user.id) do %>
                  <span class="glyphicon glyphicon-pencil"></span>
                <% end %>
            </td>
            <td>
                <%= link_to dashboard_users_destroy_path(:id => user.id) do %>
                  <span class="glyphicon glyphicon-remove"></span>
                <% end %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <br>
  </div>
</div>
